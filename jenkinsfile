pipeline{
    agent {
    node {
          label 'Agent1'
    }
    }
    stages {
        stage('Build') {
            steps {
                git 'https://github.com/pradnyeo/webapp.git'
                sh 'mvn install'
            }
        }
        stage('Deploy'){
            steps {
                sh 'cp ./target/WebApp.war /home/ec2-user/apache-tomcat-9.0.68/webapps'
            }
        }
    }
}
